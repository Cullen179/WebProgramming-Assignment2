<h1>Home vendor</h1>
<br />

<% for (let product of products)  {%>
    <p> 
        OwnershipId: <b><%=product.ownership%></b>
        <br/>
        <br/>

        Name: <b><%=product.name%></b>
        <br/>
        <br/>

        Price: <%=product.price%>
        <br/>
        <br/>

        Quantity: <%=product.quantity%>
        <br/>
        <br/>

        Description: <%=product.description%>
        <br/>
        <br/>

        Picture: <img src="<%=product.imgSrc%>" alt="Product picture">
        <br/>
        <br/>

        <a href="/product/<%=product._id%>/edit">Edit</a>
    </p>
    <hr>
<% } %>



